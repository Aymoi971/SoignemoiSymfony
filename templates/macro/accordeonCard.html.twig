{% macro card(categoryName,subcategoryName,subcategory) %}
    <div class="card">
        <div class="card-heading">
            <a data-toggle="collapse" data-target="#{{subcategoryName}}">
                {{ subcategoryName }}
            </a>
        </div>
        <div id="{{subcategoryName}}" class="collapse" data-parent="#accordion{{categoryName}}">
            <div class="card-body">
                <ul>
                    {% for item in subcategory %}
                        <li><a href="#">{{item}}</a></li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{% endmacro %}